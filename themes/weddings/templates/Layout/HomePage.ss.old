<div class="bg" id="rotate">
	<% loop $Children %>
	<div <% if First %>class="ui-tabs-panel"<% else %>class="ui-tabs-panel ui-tabs-hide"<% end_if %>id="tab$Pos">
		<a href="http://imu.uiowa.edu/weddings/" target="_blank"><img class="bg" src="$FeatureImage.URL" /></a>
		<div id="shadow2"></div>
		<div id="above-wrapper"><div id="above"><img style="float:right;margin-top:40px;margin-right:125px;" src="$ThemeDir/images/logo5.png" />
			<h1><a href="$LocationLink">$LocationTitle</a></h1>
		<p>$LocationText </p></div> </div> </div>
		<% end_loop %>
		<div id="shadow"></div>
		<ul class="ui-tabs-nav">
			<div class="thumbnails">
				<% loop $Children %>
				<li <% if First %>class="ui-tabs-selected"<% end_if %>><a href="#tab$Pos">$Pos</a></li>
				<% end_loop %>
			</div>
		</ul>
	</div>
</div>