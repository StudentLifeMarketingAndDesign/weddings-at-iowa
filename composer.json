{
    "name": "silverstripe/installer",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=5.3.2",
        "silverstripe/recipe-cms": "^1",
        "silverstripe/framework": "4.0.3",
        "silverstripe-themes/simple": "*",
		"silverstripe/userforms": "*",
        "silverstripe/tagfield": "^2.0@dev",
        "undefinedoffset/sortablegridfield": "^2.0@dev",
        "colymba/gridfield-bulk-editing-tools": "^3.0@dev",
        "jonom/focuspoint": "^3.0@dev",
        "symbiote/silverstripe-gridfieldextensions": "^3",
        "jonom/silverstripe-betternavigator" : "master"
    },
    "config": {
        "process-timeout": 600
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/division-bar"
        },
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/division-analytics"
        },
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/silverstripe-activedirectory"
        },
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/silverstripe-betternavigator"
        }
    ],
    "extra": {
        "project-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "mysite/.htaccess",
            "mysite/_config.php",
            "mysite/_config/mysite.yml",
            "mysite/code/Page.php",
            "mysite/code/PageController.php",
            "web.config"
        ]
    }
}
